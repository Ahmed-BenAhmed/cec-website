{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./frontend/dist",
    "rootDir": "./frontend/src",
    "sourceMap": true
  },
  "include": ["frontend/src/**/*.ts"],
  "exclude": [
    "frontend/node_modules"
  ]
}